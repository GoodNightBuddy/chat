model Product {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @updatedAt
  title     String
  price     Float
}

datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}



generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma-client-js"
}
